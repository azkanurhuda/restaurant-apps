{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-alert": "off",
        "no-plusplus": "off",
        "no-restricted-globals": "off",
        "import/no-extraneous-dependencies" : "off",
        "no-underscore-dangle": [2, { "allow": [], "allowAfterThis": true }]
    }
}
